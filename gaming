# Wave 11
wave_11_gaming <- k_indresp[, vars_k_gaming]

# Wave 12
wave_12_gaming <- l_indresp[, vars_l_gaming]

# Wave 13
wave_13_gaming <- m_indresp[, vars_m_gaming]

# Wave 14
wave_14_gaming <- n_indresp[, vars_n_gaming]

wave_11_gaming$wave <- 11
wave_12_gaming$wave <- 12
wave_13_gaming$wave <- 13
wave_14_gaming$wave <- 14
wave_11_gaming <- clean_names(wave_11_gaming)
wave_12_gaming <- clean_names(wave_12_gaming)
wave_13_gaming <- clean_names(wave_13_gaming)
wave_14_gaming <- clean_names(wave_14_gaming)


long_data_gaming <- bind_rows(
wave_11_gaming, wave_12_gaming, wave_13_gaming, wave_14_gaming
)

long_data_gaming <- long_data_gaming %>%
  select(pidp, wave, everything())



#### NAing for employment
long_data_gaming$employ[long_data_gaming$employ == -9] <- NA
long_data_gaming$employ[long_data_gaming$employ == -8] <- NA
long_data_gaming$employ[long_data_gaming$employ == -7] <- NA
long_data_gaming$employ[long_data_gaming$employ == -2] <- NA
