#NAing how often feel lonely
long_data_gaming$sclonely[long_data_gaming$sclonely %in% c(-9, -8, -7, -2, -1)] <- NA

# Find all variable names containing "m_hcondnew"
hcondnew_vars <- grep("m_hcondnew", names(long_data_gaming), value = TRUE)

# Replace values -8, -7, -2, -1 with NA across those variables
for (var in hcondnew_vars) {
  long_data_gaming[[var]][long_data_gaming[[var]] %in% c(-8, -7, -2, -1)] <- NA
}


long_data_gaming$ethn_dv[long_data_gaming$ethn_dv %in% c(-9)] <- NA
long_data_gaming$ethn_dv[long_data_gaming$ethn_dv %in% c(97)] <- 18
long_data_gaming$sex[long_data_gaming$sex %in% c(-9)] <- NA
long_data_gaming$sclfsat1[long_data_gaming$sclfsat1 %in% c(-9, -8, -7, -2, -1)] <- NA
